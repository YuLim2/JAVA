package ch13;

public class BookArrayTest {

	public static void main(String[] args) {
		Book library[] = new Book[3];
		
		library[0] = new Book("서유기", "킹유림");
		library[1] = new Book("용의자식", "킹유림");
		library[2] = new Book("용의딸", "킹유림");
		
		for(Book book : library) {
			book.showInfo();
		}
	}

}
